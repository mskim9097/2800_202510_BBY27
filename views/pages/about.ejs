    <style>
        body {
            font-family: sans-serif;
            margin: 20px;
            line-height: 1.6;
        }

        h1 {
            text-align: center;
        }

        h2 {
            border-bottom: 1px solid #ccc;
            padding-bottom: 5px;
            margin-top: 30px;
        }

        .dependency {
            margin-bottom: 20px;
            padding: 10px;
            border: 1px solid #eee;
            border-radius: 5px;
        }

        .dependency strong {
            display: inline-block;
            width: 100px;
        }

        .dependency ul {
            list-style-type: none;
            padding-left: 0;
        }

        .dependency li {
            margin-bottom: 5px;
        }
        #content {
            -ms-overflow-style: none;
            scrollbar-width: none;
            overflow-y: scroll;
        }

        #content::-webkit-scrollbar {
            display: none;
        }
        .line-clamp-2 {
            overflow: hidden;
            display: -webkit-box;
            -webkit-line-clamp: 2;
            -webkit-box-orient: vertical;
            text-overflow: ellipsis;
        }
    </style>
</head>

