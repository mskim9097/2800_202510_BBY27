<div style="display: flex; flex-wrap: wrap; gap: 10px;">
    <% images.forEach(imgObj => { %>
      <div>
        <img src="<%= imgObj.image %>" alt="image" style="width: 200px; border: 1px solid #ccc;">
      </div>
    <% }) %>
</div>
<button onclick="location.href='/test/testCreateImage'">Create Image</button>